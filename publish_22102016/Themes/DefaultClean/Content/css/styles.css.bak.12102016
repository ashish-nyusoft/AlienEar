        height: 150px;
        border: 1px solid #ddd;
        overflow: hidden;
    }

        .topic-post .avatar img,
        .profile-info-box .avatar img {
            position: absolute;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            max-width: 100%;
            max-height: 100%;
            margin: auto;
        }

        .topic-post .avatar img {
            border: solid 2px #CEDBE1 !important;
        }

    .topic-post .user-stats {
        margin: 0 0 15px;
