        border-bottom: 1px solid #ddd;
    }

    .topic-post:after {
        content: "";
        display: block;
        clear: both;
    }

    .topic-post .post-info {
        margin: 30px 0;
    }

    .topic-post .user-info {
        width: 150px;
        margin: 0 auto 15px;
    }

    .topic-post .username {
        display: block;
        margin: 0 0 -1px;
        border: 1px solid #ddd;
        background-color: #f6f6f6;
        padding: 11px 0;
        font-weight: bold;
        color: #444;
    }

