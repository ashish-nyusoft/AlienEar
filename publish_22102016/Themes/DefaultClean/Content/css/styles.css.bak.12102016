        .profile-info-box .profile-stats span {
            margin: 0 0 0 3px;
            font-weight: bold;
            color: #444;
        }

    .topic-post .pm-button,
    .profile-info-box .pm-button {
        display: inline-block;
        border: none;
        background-color: #888;
        padding: 10px 20px;
        font-size: 12px;
        color: #fff;
        text-transform: uppercase;
    }

